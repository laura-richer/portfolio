{
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "parser": "flow",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "singleQuote": true,
  "printWidth": 100,
  "semi": true,
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2
}
